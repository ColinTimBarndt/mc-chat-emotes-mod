<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root type="GridPane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" hgap="4">
    <columnConstraints>
        <ColumnConstraints hgrow="NEVER" minWidth="10.0"/>
        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES"/>
    </rowConstraints>
    <Label text="Name" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <ChoiceBox fx:id="nameChoice" GridPane.columnIndex="1" GridPane.rowIndex="0"/>

    <Label text="Category" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
    <ChoiceBox fx:id="categoryChoice" GridPane.columnIndex="1" GridPane.rowIndex="1"/>

    <Label text="Aliases" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
    <ChoiceBox fx:id="aliasesChoice" GridPane.columnIndex="1" GridPane.rowIndex="2"/>

    <Label text="Emoticons" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
    <ChoiceBox fx:id="emoticonsChoice" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
</fx:root>
